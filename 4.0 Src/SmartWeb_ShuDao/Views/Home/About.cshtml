@{
    ViewBag.Title = "About";
    Layout = "~/Views/Shared/_Main.cshtml";
}
<div id="av_section_1" class="avia-section main_color avia-section-default avia-no-shadow avia-bg-style-scroll  avia-builder-el-0  el_before_av_section  avia-builder-el-first  container_wrap fullsize">
    <div class="container">
        <main role="main" itemprop="mainContentOfPage" class="template-page content  twelve alpha units">
            <div class="post-entry post-entry-type-page post-entry-3799">
                <div class="entry-content-wrapper clearfix">
                    <section class="av_textblock_section" >
                        <div class="avia_textblock " itemprop="text">
                            <h1 style="text-align: center">关于我们</h1>
                        </div>
                    </section>
                    <div class="hr hr-short hr-center  avia-builder-el-2  el_after_av_textblock  el_before_av_textblock "><span class="hr-inner"><span class="hr-inner-style"></span></span></div>
                    <p></p><section class="av_textblock_section" >
                        <div class="avia_textblock " itemprop="text">
                            树道国际咨询盘根于上海，立足当代智力服务业发展趋势，颠覆传统思维，自主创立了独具特色的咨询式培训体系，利用 “管理咨询+培训教育”的专业能力，两两互哺、交叉互动，共同组成和谐共生的有机整体，为客户提供资本、咨询、人才的综合服务。
                            树道国际咨询是国内首家专注于企业顶层设计，打造企业医生养成系统的平台。总部位于上海，下辖上海、南京、宁波、哈尔滨、贵阳等多家分支机构，立志成为最受尊敬、最有实力的国际化培训、咨询公司；打造能够吸引、培育和激励杰出人才的优秀组织架构，是您值得信赖的成长顾问。

                            @*@if (ViewData["about"] != null)
                                {  <p>
                                   @new HtmlString(@ViewData["about"].ToString())
                                </p>}*@
                        </div>
                    </section>
                    <div style="height:30px" class="hr hr-invisible  avia-builder-el-4  el_after_av_textblock  el_before_av_button "><span class="hr-inner"><span class="hr-inner-style"></span></span></div>
                    <div class="avia-button-wrap avia-button-center  avia-builder-el-5  el_after_av_hr  avia-builder-el-last "><a href="/Product" class="avia-button avia-icon_select-yes avia-color-theme-color avia-size-large avia-position-center "><span class="avia_button_icon" aria-hidden="true" data-av_icon="" data-av_iconfont="entypo-fontello"></span><span class="avia_iconbox_title">了解内训课程体系</span></a></div>

                </div>
            </div>
